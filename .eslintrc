{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": [2, "never"],
        "eqeqeq": [2, "smart"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "no-unexpected-multiline": 2,
        "semi": [2, "never"],
        "object-curly-spacing": 0,
        "babel/object-curly-spacing": [2, "never"]
    },
    "plugins": ["babel"],
    "parser": "babel-eslint"
}